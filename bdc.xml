<bdc>
    <CriteresDeSecurite>
        <Critere>
            <Id>D</Id>
            <Intitule>Disponibilité</Intitule>
            <Description>Durée maximale admissible pour la réalisation d'un processus métier.</Description>
        </Critere>
        <Critere>
            <Id>I</Id>
            <Intitule>Intégrité</Intitule>
            <Description>Altération admissible et/ou mesures de récupération des données à mettre en œuvre pour un processus métier.</Description>
        </Critere>
        <Critere>
            <Id>C</Id>
            <Intitule>Confidentialité</Intitule>
            <Description>Sensibilité des informations liées à un processus métier.</Description>
        </Critere>
        <Critere>
            <Id>T</Id>
            <Intitule>Traçabilité</Intitule>
            <Description>Suivi des actions liées à un processus métier.</Description>
        </Critere>
    </CriteresDeSecurite>
    <Metriques>        
        <Metrique>
            <Critere>Disponibilité</Critere>
            <Niveaux>
                <Niveau>
                    <Numero>1</Numero>
                    <Intitule>Faible</Intitule>
                    <Description>Une indisponibilité peut être tolérée sur un intervalle de temps de quelques jours, voire plus.</Description>
                </Niveau>
                <Niveau>
                    <Numero>2</Numero>
                    <Intitule>Modérée</Intitule>
                    <Description>L’indisponibilité ne peut excéder l’ordre de quelques heures.</Description>
                </Niveau>
                <Niveau>
                    <Numero>3</Numero>
                    <Intitule>Forte</Intitule>
                    <Description>L’indisponibilité ne peut excéder l’ordre de quelques minutes.</Description>
                </Niveau>
                <Niveau>
                    <Numero>4</Numero>
                    <Intitule>Immédiate</Intitule>
                    <Description>L’indisponibilité ne peut excéder l’ordre de quelques secondes.</Description>
                </Niveau>
            </Niveaux>
        </Metrique>
        <Metrique>
            <Critere>Intégrité</Critere>
            <Niveaux>
                <Niveau>
                    <Numero>1</Numero>
                    <Intitule>Non contrôlée</Intitule>
                    <Description>Aucune mesure particulière ne doit être mise en œuvre.</Description>
                </Niveau>
                <Niveau>
                    <Numero>2</Numero>
                    <Intitule>Identifiée</Intitule>
                    <Description>Le processus essentiel peut être altéré si cette alétration est identifiée.</Description>
                </Niveau>
                <Niveau>
                    <Numero>3</Numero>
                    <Intitule>Corrigée</Intitule>
                    <Description>Le processus essentiel peut être altéré si cette altération est identifiée et l’intégrité du processus essentiel peut être retrouvée.</Description>
                </Niveau>
                <Niveau>
                    <Numero>4</Numero>
                    <Intitule>Non altérée</Intitule>
                    <Description>Le processus essentiel ne doit pas être altéré.</Description>
                </Niveau>
            </Niveaux>
        </Metrique>
        <Metrique>
            <Critere>Confidentialité</Critere>
            <Niveaux>
                <Niveau>
                    <Numero>1</Numero>
                    <Intitule>Libre</Intitule>
                    <Description>Aucune mesure particulière ne doit être mise en œuvre.</Description>
                </Niveau>
                <Niveau>
                    <Numero>2</Numero>
                    <Intitule>Entreprise</Intitule>
                    <Description>La connaissance s'organise sur un périmètre d'accès à la maille d'ERDF.</Description>
                </Niveau>
                <Niveau>
                    <Numero>3</Numero>
                    <Intitule>Restreint</Intitule>
                    <Description>La connaissance est limitée à des personnes, fonctions ou à un périmètre restreint lié à une activité.</Description>
                </Niveau>
                <Niveau>
                    <Numero>4</Numero>
                    <Intitule>Confidentiel</Intitule>
                    <Description>Ne doit être connu que par des personnes nommément désignées et autorisées à cet effet.</Description>
                </Niveau>
            </Niveaux>
        </Metrique>
        <Metrique>
            <Critere>Traçabilité</Critere>
            <Niveaux>
                <Niveau>
                    <Numero>1</Numero>
                    <Intitule>Inexistante</Intitule>
                    <Description>La traçabiltié n'est pas nécessaire.</Description>
                </Niveau>
                <Niveau>
                    <Numero>2</Numero>
                    <Intitule>Courte</Intitule>
                    <Description>La traçabilité doit être assurée et conservée pour une durée de quelques mois.</Description>
                </Niveau>
                <Niveau>
                    <Numero>3</Numero>
                    <Intitule>Moyenne</Intitule>
                    <Description>La traçabilité doit être assurée et conservée pour une durée de quelques années.</Description>
                </Niveau>
                <Niveau>
                    <Numero>4</Numero>
                    <Intitule>Longue</Intitule>
                    <Description>La traçabilité doit être assurée et conservée pendant de nombreuses années voir indéfiniment.</Description>
                </Niveau>
            </Niveaux>
        </Metrique>
        <Metrique>
            <Critere>Gravite</Critere>
            <Niveaux>
                <Niveau>
                    <Numero>1</Numero>
                    <Intitule>Négligeable</Intitule>
                    <Description>Gravite négligeable</Description>
                </Niveau>
                <Niveau>
                    <Numero>2</Numero>
                    <Intitule>Limitée</Intitule>
                    <Description>Gravite limitée</Description>
                </Niveau>
                <Niveau>
                    <Numero>3</Numero>
                    <Intitule>Importante</Intitule>
                    <Description>Gravite importante</Description>
                </Niveau>
                <Niveau>
                    <Numero>4</Numero>
                    <Intitule>Critique</Intitule>
                    <Description>Gravite critique</Description>
                </Niveau>
            </Niveaux>
        </Metrique>
        <Metrique>
            <Critere>Vraisemblance</Critere>
            <Niveaux>
                <Niveau>
                    <Numero>1</Numero>
                    <Intitule>Minime</Intitule>
                    <Description>Minime</Description>
                </Niveau>
                <Niveau>
                    <Numero>2</Numero>
                    <Intitule>Significative</Intitule>
                    <Description>Significative</Description>
                </Niveau>
                <Niveau>
                    <Numero>3</Numero>
                    <Intitule>Forte</Intitule>
                    <Description>Forte</Description>
                </Niveau>
                <Niveau>
                    <Numero>4</Numero>
                    <Intitule>Maximale</Intitule>
                    <Description>Maximale</Description>
                </Niveau>
            </Niveaux>
        </Metrique>
    </Metriques>
